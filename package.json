{
  "name": "nuxt-pwa-starter",
  "version": "1.0.0",
  "description": "Nuxt.js PWA starter template",
  "keywords": [
    "nuxt",
    "pwa",
    "vite",
    "starter"
  ],
  "private": true,
  "type": "module",
  "scripts": {
